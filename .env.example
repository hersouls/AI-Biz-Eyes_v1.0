# 서버 설정
PORT=3003
NODE_ENV=development

# 데이터베이스 설정
DATABASE_URL=postgresql://username:password@localhost:5432/ai_biz_eyes

# JWT 설정
JWT_SECRET=your-jwt-secret-key-here
JWT_EXPIRES_IN=24h

# CORS 설정
ALLOWED_ORIGINS=http://localhost:3000,https://bizeyes.moonwave.kr

# 조달청 API 설정
G2B_SERVICE_KEY=your-g2b-service-key-here

# 파일 업로드 설정
MAX_FILE_SIZE=10485760
UPLOAD_PATH=./uploads

# 로깅 설정
LOG_LEVEL=info
LOG_FILE=./logs/app.log